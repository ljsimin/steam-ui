<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>SteamUiEndpoint</servlet-name>
		<servlet-class>net.shiny.steamui.endpoint.SteamUiEndpoint</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GameDetailEndpoint</servlet-name>
		<servlet-class>net.shiny.steamui.endpoint.GameDetailEndpoint</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GameGenresEndpoint</servlet-name>
		<servlet-class>net.shiny.steamui.endpoint.GameGenresEndpoint</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PlayerDetailEndpoint</servlet-name>
		<servlet-class>net.shiny.steamui.endpoint.PlayerDetailEndpoint</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TagEndpoint</servlet-name>
		<servlet-class>net.shiny.steamui.endpoint.TagEndpoint</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WarmupServlet</servlet-name>
		<servlet-class>net.shiny.steamui.servlet.WarmupServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>SteamUiEndpoint</servlet-name>
		<url-pattern>/steamui</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GameDetailEndpoint</servlet-name>
		<url-pattern>/game</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GameGenresEndpoint</servlet-name>
		<url-pattern>/genres</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PlayerDetailEndpoint</servlet-name>
		<url-pattern>/player</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TagEndpoint</servlet-name>
		<url-pattern>/tags</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value/>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>
	<filter>
  		<filter-name>appstats</filter-name>
   		<filter-class>com.google.appengine.tools.appstats.AppstatsFilter</filter-class>
	</filter>

	<filter-mapping>
  		<filter-name>appstats</filter-name>
  		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
	<servlet>
  		<servlet-name>appstats</servlet-name>
  		<servlet-class>com.google.appengine.tools.appstats.AppstatsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
  		<servlet-name>appstats</servlet-name>
  		<url-pattern>/appstats/*</url-pattern>
	</servlet-mapping>
	
	<security-constraint>
  		<web-resource-collection>
    		<web-resource-name>appstats</web-resource-name>
    		<url-pattern>/appstats/*</url-pattern>
  		</web-resource-collection>
  		<auth-constraint>
    		<role-name>admin</role-name>
  		</auth-constraint>
	</security-constraint>
</web-app>